{
	"name": "@obelism/improve-sdk",
	"description": "Obelism Improve SDK",
	"version": "0.3.4",
	"keywords": [
		"ab-tests",
		"feature-flags",
		"typescript",
		"improve"
	],
	"license": "MIT",
	"author": "Obelism <hello@obelism.studio> (https://www.obelism.studio)",
	"repository": {
		"type": "git",
		"url": "https://github.com/Obelism/improve-sdk"
	},
	"main": "./src/server.ts",
	"files": [
		"dist"
	],
	"exports": {
		"./server": {
			"types": "./dist/server.d.ts",
			"import": "./dist/server.mjs",
			"require": "./dist/server.cjs"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.mjs",
			"require": "./dist/client.cjs"
		},
		"./types": {
			"types": "./dist/types.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"server": [
				"./dist/server.d.ts"
			],
			"client": [
				"./dist/client.d.ts"
			],
			"types": [
				"./dist/types.d.ts"
			]
		}
	},
	"scripts": {
		"build": "bunchee -m",
		"dev": "bunchee --watch --no-clean",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ci": "vitest run",
		"lint": "npm-run-all --parallel lint:*",
		"lint:fix": "eslint --fix",
		"lint:types": "tsc --noEmit",
		"clean": "rm -rf ./node_modules && rm -rf ./dist && rm -rf ./.turbo"
	},
	"peerDependencies": {
		"ua-parser-js": "^1.0.38"
	},
	"devDependencies": {
		"@obelism/improve-sdk-eslint-config": "^0.0.0",
		"@obelism/improve-sdk-typescript-config": "^0.0.0",
		"@types/ua-parser-js": "^0.7.39",
		"@typescript-eslint/eslint-plugin": "^8.1.0",
		"bunchee": "^5.3.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-no-relative-import-paths": "^1.5.3",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"lint-staged": "^15.2.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.3.3",
		"vitest": "^2.1.5"
	}
}
