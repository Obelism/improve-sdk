{
	"name": "@obelism/improve-sdk-next",
	"description": "Obelism Improve NextJS SDK",
	"version": "0.3.5",
	"keywords": [
		"ab-tests",
		"feature-flags",
		"typescript",
		"improve",
		"react",
		"nextjs"
	],
	"license": "MIT",
	"author": "Obelism <hello@obelism.studio> (https://www.obelism.studio)",
	"repository": {
		"type": "git",
		"url": "https://github.com/Obelism/improve-sdk-next"
	},
	"files": [
		"dist"
	],
	"exports": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "bunchee -m",
		"dev": "bunchee --watch --no-clean",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ci": "vitest run",
		"lint": "npm-run-all --parallel lint:*",
		"lint:fix": "eslint --fix",
		"lint:types": "tsc --noEmit",
		"clean": "rm -rf ./node_modules && rm -rf ./dist && rm -rf ./.turbo"
	},
	"peerDependencies": {
		"next": ">=14.0.0",
		"react": ">=18.0.0"
	},
	"devDependencies": {
		"@obelism/improve-sdk-eslint-config": "^0.0.0",
		"@obelism/improve-sdk-typescript-config": "^0.0.0",
		"@types/react": "^18.2.52",
		"@typescript-eslint/eslint-plugin": "^8.1.0",
		"bunchee": "^6.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-no-relative-import-paths": "^1.5.3",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"typescript": "^5.3.3",
		"vitest": "^2.0.5"
	},
	"dependencies": {
		"@obelism/improve-sdk": "^0.3.5",
		"@obelism/improve-sdk-react": "^0.3.5"
	}
}
